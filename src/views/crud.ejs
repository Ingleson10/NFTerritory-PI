<body>
    <header>
        <div class="title">
            <h3>Modelo CRUD</h3>
         </div>
        <div class="rotas"> 
            <a href="/"><span> Home </span></a>
            <a href="/produtos"><span> Produtos</span></a>
        </div>
         <div class="jessica">
            <!-- <img src="../../public/img/jessicaCrud.jpg" alt="jessica"> -->
